{
  "name": "markdown-wrangler",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.38.0",
    "eslint": "9.38.0",
    "globals": "16.4.0",
    "playwright": "^1.58.2",
    "prettier": "^3.6.2"
  },
  "scripts": {
    "lint": "eslint static/*.js",
    "format": "prettier --write static/*.js",
    "check": "eslint static/*.js && prettier --check static/*.js"
  }
}